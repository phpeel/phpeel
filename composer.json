{
    "name": "phpingguo/phpingguo",
    "description": "The Phpingguo PHP framework.",
    "license": "MIT",
    "keywords": [ "framework" ],
    "require": {
        "php": ">=5.4.0",
        "phpingguo/apricot-lib": "dev-develop",
        "phpingguo/citron-di": "dev-develop",
        "phpingguo/banana-validator": "dev-develop"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "squizlabs/php_codesniffer": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Phpingguo\\System\\": "src/phpingguo/",
            "Phpingguo\\App\\": "src/app/",
            "Phpingguo\\System\\Tests\\": "tests/phpingguo/"
        }
    }
}
