{
    "name": "phpeel/phpeel",
    "description": "The Phpeel PHP framework.",
    "license": "MIT",
    "keywords": [ "framework" ],
    "require": {
        "php": ">=5.4.0",
        "phpeel/apricot-lib": "dev-develop",
        "phpeel/citron-di": "dev-develop",
        "phpeel/banana-validator": "dev-develop",
        "phpeel/uzuki-xml": "dev-develop",
        "twig/twig": "~1.0",
        "smarty/smarty": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "squizlabs/php_codesniffer": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Phpeel\\System\\": "src/phpeel/",
            "Phpeel\\App\\": "src/app/",
            "Phpeel\\System\\Tests\\": "tests/phpeel/"
        }
    }
}
